<Ui xmlns="https://www.blizzard.com/wow/ui/" 
 xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="https://www.blizzard.com/wow/ui/
 ..\..\FrameXML\UI.xsd">

	<!-- event listener -->
	<Frame name="_hybar_listener">
		<Scripts>
			<OnLoad>
				self:RegisterEvent("ADDON_LOADED");
			</OnLoad>
			<OnEvent>
				_hybar.OnEvent (self, event, ...);
			</OnEvent>
		</Scripts>
	</Frame>
</Ui> 
